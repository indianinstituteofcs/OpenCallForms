<!-- Dress Size -->                        
<div class="form-group form-row " style="margin-top:2rem;">
    <div class="col input-group">
        <div class="input-group-prepend">
            <span class="input-group-text" for="inputDressSize">Dress Size</span>
        </div>
        <% if((typeof userDressSize === 'undefined') || (userDressSize === null)){ %>
            <input class="form-control" type="number" id="inputDressSize" name="dressSize" min="0" max="32" placeholder="0 to 32" required>
