            <input class="form-control" type="number" id="inputDressSize" name="dressSize" min="0" max="32" value=<%= userDressSize; %> required>
        <% }; %>      
    </div>
</div>

<!-- Height -->                        
<div class="form-group form-row justify-content-center" style="margin-top:2rem;">
    <label class="col-xs-3 col-form-label text-right">Height:</label>
    <div class="col input-group">
        <div class="input-group-prepend">
            <span class="input-group-text" for="inputHeightFeet">Feet</span>
        </div>
        <% if((typeof userHeightFeet === 'undefined') || (userHeightFeet === null)){ %>
            <input class="form-control" type="number" id="inputHeightFeet" name="htFeet" min="5" max="6" placeholder="Feet.." required>
        <% } else { %>
            <input class="form-control" type="number" id="inputHeightFeet" name="htFeet" min="5" max="6" value=<%= userHeightFeet; %> required>
        <% }; %>  
    </div>
    <div class="col input-group">
        <div class="input-group-prepend">
            <span class="input-group-text" for="inputHeightInches">Inches</span>
        </div>
        <% if((typeof userHeightInches === 'undefined') || (userHeightInches === null)){ %>
            <input class="form-control" type="number" id="inputHeightInches" name="htInches" min="0" max="11" placeholder="Inches.." required>
        <% } else { %>
            <input class="form-control" type="number" id="inputHeightInches" name="htInches" min="0" max="11" value=<%= userHeightInches; %> required>
        <% }; %>  
    </div>
</div> 
              
