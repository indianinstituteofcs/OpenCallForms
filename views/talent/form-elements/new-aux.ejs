<!-- COVID 19 form -->            
<div class="justify-content-center" style="margin-top:2rem;">
    <img src="/images/COVID19LiabilityWaiver.jpg" style="width:100%" alt="Home">
</div>

<div class="form-group form-row justify-content-center" style="margin-top:2rem;">
    <div class="col input-group">
        <div class="input-group-prepend">
            <span class="input-group-text" for="covidSignatureId">Signature</span>
        </div>
        <% if((typeof userCovidSignature === 'undefined') || (userCovidSignature === null)){ %>
            <textarea type="text" class="form-control" id="covidSignatureId" name="covidSignature" rows=1 placeholder="Type first & last name.." required></textarea>                       
        <% } else { %>
            <textarea type="text" class="form-control" id="covidSignatureId" name="covidSignature" rows=1 required><%= userCovidSignature; %></textarea>                     
        <% }; %> 
     </div>
</div>

<!-- Confirm agreement to COVID DISCLOSURE -->
<div class="form-group form-row">
    <div class="col" >
        <select class="form-control selectpicker" id="checkCovidId" name="covidCheck" title="Do you agree with the COVID-19 DISCLOSURE?" required>
            <% if((typeof userCovidCheck === 'undefined') || (userCovidCheck === null)){ %>
                <option value="0">I have read the COVID-19 DISCLOSURE, and I agree with it</option>
                <option value="1" >I have read the COVID-19 DISCLOSURE and DO NOT agree with it</option>
            <% } else { %>
                <option value="0" <% if (userCovidCheck == "0") {%> selected <%}%>>I have read the COVID-19 DISCLOSURE, and I agree with it</option>
                <option value="1" <% if (userCovidCheck == "1") {%> selected <%}%>>I have read the COVID-19 DISCLOSURE and DO NOT agree with it</option>
            <% }; %> 
        </select>     
    </div>
</div>

